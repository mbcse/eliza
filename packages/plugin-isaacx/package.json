{
  "name": "@elizaos-plugins/plugin-isaacx",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/core": "^0.25.6-alpha.1"
  },
  "devDependencies": {
    "tsup": "8.3.5",
    "vitest": "^3.0.0"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "ISAACX_API_KEY": {
        "type": "string",
        "description": "API key for Isaac X service"
      }
    }
  }
}